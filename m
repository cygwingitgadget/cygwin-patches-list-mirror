+# as_fn_error STATUS ERROR [LINENO LOG_FD]=0A=
+# ----------------------------------------=0A=
+# Output "`basename $0`: error: ERROR" to stderr. If LINENO and LOG_FD are=
=0A=
+# provided, also output the error to LOG_FD, referencing LINENO. Then exit=
 the=0A=
+# script with STATUS, using 1 if that was 0.=0A=
+as_fn_error ()=0A=
+{=0A=
+  as_status=3D$1; test $as_status -eq 0 && as_status=3D1=0A=
+  if test "$4"; then=0A=
+    as_lineno=3D${as_lineno-"$3"} as_lineno_stack=3Das_lineno_stack=3D$as_=
lineno_stack=0A=
+    $as_echo "$as_me:${as_lineno-$LINENO}: error: $2" >&$4=0A=
+  fi=0A=
+  $as_echo "$as_me: error: $2" >&2=0A=
+  as_fn_exit $as_status=0A=
+} # as_fn_error=0A=
+=0A=
+=0A=
+# as_fn_set_status STATUS=0A=
+# -----------------------=0A=
+# Set $? to STATUS, without forking.=0A=
+as_fn_set_status ()=0A=
+{=0A=
+  return $1=0A=
+} # as_fn_set_status=0A=
+=0A=
+# as_fn_exit STATUS=0A=
+# -----------------=0A=
+# Exit the shell with STATUS, even in a "trap 0" or "set -e" context.=0A=
+as_fn_exit ()=0A=
+{=0A=
+  set +e=0A=
+  as_fn_set_status $1=0A=
+  exit $1=0A=
+} # as_fn_exit=0A=
+=0A=
+# as_fn_unset VAR=0A=
+# ---------------=0A=
+# Portably unset VAR.=0A=
+as_fn_unset ()=0A=
+{=0A=
+  { eval $1=3D; unset $1;}=0A=
+}=0A=
+as_unset=3Das_fn_unset=0A=
+# as_fn_append VAR VALUE=0A=
+# ----------------------=0A=
+# Append the text in VALUE to the end of the definition contained in VAR. =
Take=0A=
+# advantage of any shell optimizations that allow amortized linear growth =
over=0A=
+# repeated appends, instead of the typical quadratic growth present in nai=
ve=0A=
+# implementations.=0A=
+if (eval "as_var=3D1; as_var+=3D2; test x\$as_var =3D x12") 2>/dev/null; t=
hen :=0A=
+  eval 'as_fn_append ()=0A=
+  {=0A=
+    eval $1+=3D\$2=0A=
+  }'=0A=
+else=0A=
+  as_fn_append ()=0A=
+  {=0A=
+    eval $1=3D\$$1\$2=0A=
+  }=0A=
