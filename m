+=0A=
+# ac_fn_c_try_link LINENO=0A=
+# -----------------------=0A=
+# Try to link conftest.$ac_ext, and return whether this succeeded.=0A=
+ac_fn_c_try_link ()=0A=
+{=0A=
+  as_lineno=3D${as_lineno-"$1"} as_lineno_stack=3Das_lineno_stack=3D$as_li=
neno_stack=0A=
+  rm -f conftest.$ac_objext conftest$ac_exeext=0A=
+  if { { ac_try=3D"$ac_link"=0A=
+case "(($ac_try" in=0A=
+  *\"* | *\`* | *\\*) ac_try_echo=3D\$ac_try;;=0A=
+  *) ac_try_echo=3D$ac_try;;=0A=
+esac=0A=
+eval ac_try_echo=3D"\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""=0A=
+$as_echo "$ac_try_echo"; } >&5=0A=
+  (eval "$ac_link") 2>conftest.err=0A=
+  ac_status=3D$?=0A=
+  if test -s conftest.err; then=0A=
+    grep -v '^ *+' conftest.err >conftest.er1=0A=
+    cat conftest.er1 >&5=0A=
+    mv -f conftest.er1 conftest.err=0A=
+  fi=0A=
+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? =3D $ac_status" >&5=0A=
+  test $ac_status =3D 0; } && {=0A=
+	 test -z "$ac_c_werror_flag" ||=0A=
+	 test ! -s conftest.err=0A=
+       } && test -s conftest$ac_exeext && {=0A=
+	 test "$cross_compiling" =3D yes ||=0A=
+	 $as_test_x conftest$ac_exeext=0A=
+       }; then :=0A=
+  ac_retval=3D0=0A=
+else=0A=
+  $as_echo "$as_me: failed program was:" >&5=0A=
+sed 's/^/| /' conftest.$ac_ext >&5=0A=
+=0A=
+	ac_retval=3D1=0A=
+fi=0A=
+  # Delete the IPA/IPO (Inter Procedural Analysis/Optimization) informatio=
n=0A=
+  # created by the PGI compiler (conftest_ipa8_conftest.oo), as it would=
=0A=
+  # interfere with the next link command; also delete a directory that is=
=0A=
+  # left behind by Apple's compiler.  We do this before executing the acti=
ons.=0A=
+  rm -rf conftest.dSYM conftest_ipa8_conftest.oo=0A=
+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno=0A=
+  as_fn_set_status $ac_retval=0A=
+=0A=
+} # ac_fn_c_try_link=0A=
+=0A=
+# ac_fn_c_check_func LINENO FUNC VAR=0A=
+# ----------------------------------=0A=
+# Tests whether FUNC exists, setting the cache variable VAR accordingly=0A=
+ac_fn_c_check_func ()=0A=
+{=0A=
+  as_lineno=3D${as_lineno-"$1"} as_lineno_stack=3Das_lineno_stack=3D$as_li=
neno_stack=0A=
+  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for $2" >&5=0A=
+$as_echo_n "checking for $2... " >&6; }=0A=
+if eval \${$3+:} false; then :=0A=
+  $as_echo_n "(cached) " >&6=0A=
+else=0A=
+  cat confdefs.h - <<_ACEOF >conftest.$ac_ext=0A=
+/* end confdefs.h.  */=0A=
+/* Define $2 to an innocuous variant, in case <limits.h> declares $2.=0A=
+   For example, HP-UX 11i <limits.h> declares gettimeofday.  */=0A=
+#define $2 innocuous_$2=0A=
+=0A=
+/* System header to define __stub macros and hopefully few prototypes,=0A=
+    which can conflict with char $2 (); below.=0A=
+    Prefer <limits.h> to <assert.h> if __STDC__ is defined, since=0A=
+    <limits.h> exists even on freestanding compilers.  */=0A=
+=0A=
+#ifdef __STDC__=0A=
+# include <limits.h>=0A=
+#else=0A=
+# include <assert.h>=0A=
+#endif=0A=
+=0A=
+#undef $2=0A=
+=0A=
+/* Override any GCC internal prototype to avoid an error.=0A=
+   Use char because int might match the return type of a GCC=0A=
+   builtin and then its argument prototype would still apply.  */=0A=
+#ifdef __cplusplus=0A=
+extern "C"=0A=
+#endif=0A=
+char $2 ();=0A=
+/* The GNU C library defines this for functions which it implements=0A=
+    to always fail with ENOSYS.  Some functions are actually named=0A=
+    something starting with __ and the normal name is an alias.  */=0A=
+#if defined __stub_$2 || defined __stub___$2=0A=
+choke me=0A=
+#endif=0A=
+=0A=
+int=0A=
+main ()=0A=
+{=0A=
+return $2 ();=0A=
+  ;=0A=
+  return 0;=0A=
+}=0A=
+_ACEOF=0A=
+if ac_fn_c_try_link "$LINENO"; then :=0A=
+  eval "$3=3Dyes"=0A=
+else=0A=
+  eval "$3=3Dno"=0A=
+fi=0A=
+rm -f core conftest.err conftest.$ac_objext \=0A=
+    conftest$ac_exeext conftest.$ac_ext=0A=
+fi=0A=
+eval ac_res=3D\$$3=0A=
+	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5=0A=
+$as_echo "$ac_res" >&6; }=0A=
+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno=0A=
+=0A=
+} # ac_fn_c_check_func=0A=
 cat >config.log <<_ACEOF=0A=
