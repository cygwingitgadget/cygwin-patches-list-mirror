+# ac_fn_c_try_compile LINENO=0A=
+# --------------------------=0A=
+# Try to compile conftest.$ac_ext, and return whether this succeeded.=0A=
+ac_fn_c_try_compile ()=0A=
+{=0A=
+  as_lineno=3D${as_lineno-"$1"} as_lineno_stack=3Das_lineno_stack=3D$as_li=
neno_stack=0A=
+  rm -f conftest.$ac_objext=0A=
+  if { { ac_try=3D"$ac_compile"=0A=
+case "(($ac_try" in=0A=
+  *\"* | *\`* | *\\*) ac_try_echo=3D\$ac_try;;=0A=
+  *) ac_try_echo=3D$ac_try;;=0A=
+esac=0A=
+eval ac_try_echo=3D"\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""=0A=
+$as_echo "$ac_try_echo"; } >&5=0A=
+  (eval "$ac_compile") 2>conftest.err=0A=
+  ac_status=3D$?=0A=
+  if test -s conftest.err; then=0A=
+    grep -v '^ *+' conftest.err >conftest.er1=0A=
+    cat conftest.er1 >&5=0A=
+    mv -f conftest.er1 conftest.err=0A=
+  fi=0A=
+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? =3D $ac_status" >&5=0A=
+  test $ac_status =3D 0; } && {=0A=
+	 test -z "$ac_c_werror_flag" ||=0A=
+	 test ! -s conftest.err=0A=
+       } && test -s conftest.$ac_objext; then :=0A=
+  ac_retval=3D0=0A=
+else=0A=
+  $as_echo "$as_me: failed program was:" >&5=0A=
+sed 's/^/| /' conftest.$ac_ext >&5=0A=
+=0A=
+	ac_retval=3D1=0A=
+fi=0A=
+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno=0A=
+  as_fn_set_status $ac_retval=0A=
+=0A=
+} # ac_fn_c_try_compile=0A=
