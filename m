+# ac_fn_c_try_run LINENO=0A=
+# ----------------------=0A=
+# Try to link conftest.$ac_ext, and return whether this succeeded. Assumes=
=0A=
+# that executables *can* be run.=0A=
+ac_fn_c_try_run ()=0A=
+{=0A=
+  as_lineno=3D${as_lineno-"$1"} as_lineno_stack=3Das_lineno_stack=3D$as_li=
neno_stack=0A=
+  if { { ac_try=3D"$ac_link"=0A=
+case "(($ac_try" in=0A=
+  *\"* | *\`* | *\\*) ac_try_echo=3D\$ac_try;;=0A=
+  *) ac_try_echo=3D$ac_try;;=0A=
+esac=0A=
+eval ac_try_echo=3D"\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""=0A=
+$as_echo "$ac_try_echo"; } >&5=0A=
+  (eval "$ac_link") 2>&5=0A=
+  ac_status=3D$?=0A=
+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? =3D $ac_status" >&5=0A=
+  test $ac_status =3D 0; } && { ac_try=3D'./conftest$ac_exeext'=0A=
+  { { case "(($ac_try" in=0A=
+  *\"* | *\`* | *\\*) ac_try_echo=3D\$ac_try;;=0A=
+  *) ac_try_echo=3D$ac_try;;=0A=
+esac=0A=
+eval ac_try_echo=3D"\"\$as_me:${as_lineno-$LINENO}: $ac_try_echo\""=0A=
+$as_echo "$ac_try_echo"; } >&5=0A=
+  (eval "$ac_try") 2>&5=0A=
+  ac_status=3D$?=0A=
+  $as_echo "$as_me:${as_lineno-$LINENO}: \$? =3D $ac_status" >&5=0A=
+  test $ac_status =3D 0; }; }; then :=0A=
+  ac_retval=3D0=0A=
+else=0A=
+  $as_echo "$as_me: program exited with status $ac_status" >&5=0A=
+       $as_echo "$as_me: failed program was:" >&5=0A=
+sed 's/^/| /' conftest.$ac_ext >&5=0A=
+=0A=
+       ac_retval=3D$ac_status=0A=
+fi=0A=
+  rm -rf conftest.dSYM conftest_ipa8_conftest.oo=0A=
+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno=0A=
+  as_fn_set_status $ac_retval=0A=
+=0A=
