+# as_fn_mkdir_p=0A=
+# -------------=0A=
+# Create "$as_dir" as a directory, including parents if necessary.=0A=
+as_fn_mkdir_p ()=0A=
+{=0A=
=20=0A=
+  case $as_dir in #(=0A=
+  -*) as_dir=3D./$as_dir;;=0A=
+  esac=0A=
+  test -d "$as_dir" || eval $as_mkdir_p || {=0A=
+    as_dirs=3D=0A=
+    while :; do=0A=
+      case $as_dir in #(=0A=
+      *\'*) as_qdir=3D`$as_echo "$as_dir" | sed "s/'/'\\\\\\\\''/g"`;; #'(=
=0A=
+      *) as_qdir=3D$as_dir;;=0A=
+      esac=0A=
+      as_dirs=3D"'$as_qdir' $as_dirs"=0A=
+      as_dir=3D`$as_dirname -- "$as_dir" ||=0A=
+$as_expr X"$as_dir" : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \=0A=
+	 X"$as_dir" : 'X\(//\)[^/]' \| \=0A=
