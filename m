+else=0A=
+  cat confdefs.h - <<_ACEOF >conftest.$ac_ext=0A=
+/* end confdefs.h.  */=0A=
+$4=0A=
+#include <$2>=0A=
+_ACEOF=0A=
+if ac_fn_c_try_compile "$LINENO"; then :=0A=
+  eval "$3=3Dyes"=0A=
+else=0A=
+  eval "$3=3Dno"=0A=
+fi=0A=
+rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext=0A=
+fi=0A=
+eval ac_res=3D\$$3=0A=
+	       { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ac_res" >&5=0A=
+$as_echo "$ac_res" >&6; }=0A=
+  eval $as_lineno_stack; ${as_lineno_stack:+:} unset as_lineno=0A=
+=0A=
